<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qinzy.admin.server.model.admin.mapper.LogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qinzy.admin.server.model.admin.entity.LogEntity">
        <id column="id" property="id"/>
        <result column="menu" property="menu"/>
        <result column="action" property="action"/>
        <result column="operation" property="operation"/>
        <result column="request_ip" property="requestIp"/>
        <result column="request_user_id" property="requestUserId"/>
        <result column="request_date" property="requestDate"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, menu, action, operation, request_ip, request_user_id, request_date
    </sql>

</mapper>
